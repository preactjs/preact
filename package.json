{
  "name": "preact",
  "amdName": "preact",
  "version": "11.0.0-beta.1",
  "private": false,
  "description": "Fast 4kb React-compatible Virtual DOM library.",
  "type": "module",
  "main": "dist/preact.js",
  "module": "dist/preact.esm.js",
  "source": "src/index.js",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./dist/preact.esm.js"
    },
    "./compat": {
      "types": "./compat/src/index.d.cts",
      "import": "./compat/dist/compat.esm.js"
    },
    "./debug": {
      "types": "./debug/src/index.d.ts",
      "import": "./debug/dist/debug.esm.js"
    },
    "./devtools": {
      "types": "./devtools/src/index.d.ts",
      "import": "./devtools/dist/devtools.esm.js"
    },
    "./hooks": {
      "types": "./hooks/src/index.d.ts",
      "import": "./hooks/dist/hooks.esm.js"
    },
    "./test-utils": {
      "types": "./test-utils/src/index.d.ts",
      "import": "./test-utils/dist/testUtils.esm.js"
    },
    "./compat/test-utils": {
      "types": "./test-utils/src/index.d.ts",
      "import": "./test-utils/dist/testUtils.esm.js"
    },
    "./jsx-runtime": {
      "types": "./jsx-runtime/src/index.d.ts",
      "import": "./jsx-runtime/dist/jsxRuntime.esm.js"
    },
    "./jsx-dev-runtime": {
      "types": "./jsx-runtime/src/index.d.ts",
      "import": "./jsx-runtime/dist/jsxRuntime.esm.js"
    },
    "./compat/client": {
      "types": "./compat/client.d.ts",
      "import": "./compat/client.js"
    },
    "./compat/server": {
      "types": "./compat/server.d.ts",
      "browser": "./compat/server.browser.js",
      "import": "./compat/server.js"
    },
    "./compat/server.browser": {
      "types": "./compat/server.d.ts",
      "default": "./compat/server.browser.js"
    },
    "./compat/jsx-runtime": {
      "types": "./jsx-runtime/src/index.d.ts",
      "import": "./compat/jsx-runtime.js"
    },
    "./compat/jsx-dev-runtime": {
      "types": "./jsx-runtime/src/index.d.ts",
      "import": "./compat/jsx-dev-runtime.js"
    },
    "./compat/scheduler": {
      "types": "./compat/scheduler.d.ts",
      "import": "./compat/scheduler.js"
    },
    "./package.json": "./package.json",
    "./compat/package.json": "./compat/package.json",
    "./debug/package.json": "./debug/package.json",
    "./devtools/package.json": "./devtools/package.json",
    "./hooks/package.json": "./hooks/package.json",
    "./test-utils/package.json": "./test-utils/package.json",
    "./jsx-runtime/package.json": "./jsx-runtime/package.json"
  },
  "license": "MIT",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/preact"
  },
  "types": "src/index.d.ts",
  "scripts": {
    "prepare": "husky && npm run test:install && run-s build",
    "build": "npm-run-all --parallel 'build:*'",
    "build:core": "microbundle build --raw --no-generateTypes -f esm",
    "build:debug": "microbundle build --raw --no-generateTypes -f esm --cwd debug",
    "build:devtools": "microbundle build --raw --no-generateTypes -f esm --cwd devtools",
    "build:hooks": "microbundle build --raw --no-generateTypes -f esm --cwd hooks",
    "build:test-utils": "microbundle build --raw --no-generateTypes -f esm --cwd test-utils",
    "build:compat": "microbundle build --raw --no-generateTypes -f esm --cwd compat",
    "build:jsx": "microbundle build --raw --no-generateTypes -f esm --cwd jsx-runtime",
    "postbuild": "node ./config/compat-entries.js",
    "test": "npm-run-all build lint test:unit",
    "test:install": "playwright install chromium",
    "test:unit": "run-p test:vitest:min test:ts",
    "test:vitest": "cross-env COVERAGE=true vitest run",
    "test:vitest:min": "cross-env MINIFY=true vitest run",
    "test:vitest:watch": "vitest",
    "test:ts": "run-p 'test:ts:*'",
    "test:ts:core": "tsc -p test/ts/",
    "test:ts:compat": "tsc -p compat/test/ts/",
    "lint": "run-s oxlint tsc",
    "tsc": "tsc -p jsconfig-lint.json",
    "oxlint": "oxlint src test/browser test/node test/shared debug compat hooks test-utils",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "postinstall": "patch-package"
  },
  "nano-staged": {
    "**/*.{js,jsx,mjs,cjs,ts,tsx,yml,json,html,md,css,scss}": [
      "biome format --write --no-errors-on-unmatched"
    ]
  },
  "files": [
    "src",
    "dist",
    "compat/dist",
    "compat/src",
    "compat/client.d.ts",
    "compat/client.js",
    "compat/server.d.ts",
    "compat/server.browser.js",
    "compat/server.js",
    "compat/scheduler.d.ts",
    "compat/scheduler.js",
    "compat/test-utils.js",
    "compat/jsx-runtime.js",
    "compat/jsx-dev-runtime.js",
    "compat/package.json",
    "debug/dist",
    "debug/src",
    "debug/package.json",
    "devtools/dist",
    "devtools/src",
    "devtools/package.json",
    "hooks/dist",
    "hooks/src",
    "hooks/package.json",
    "jsx-runtime/dist",
    "jsx-runtime/src",
    "jsx-runtime/package.json",
    "test-utils/src",
    "test-utils/package.json",
    "test-utils/dist"
  ],
  "keywords": [
    "preact",
    "react",
    "ui",
    "user interface",
    "virtual dom",
    "vdom",
    "components",
    "dom diff",
    "front-end",
    "framework"
  ],
  "authors": [
    "The Preact Authors (https://github.com/preactjs/preact/contributors)"
  ],
  "repository": "preactjs/preact",
  "bugs": "https://github.com/preactjs/preact/issues",
  "homepage": "https://preactjs.com",
  "devDependencies": {
    "@actions/github": "^6.0.1",
    "@actions/glob": "^0.5.0",
    "@babel/core": "^7.28.0",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/plugin-transform-react-jsx-source": "^7.27.1",
    "@babel/preset-env": "^7.28.0",
    "@babel/register": "^7.27.1",
    "@biomejs/biome": "^2.1.2",
    "@types/node": "^18.19.87",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-plugin-transform-rename-properties": "0.1.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "errorstacks": "^2.4.1",
    "esbuild": "^0.25.8",
    "husky": "^9.1.7",
    "kolorist": "^1.8.0",
    "microbundle": "^0.15.1",
    "npm-run-all2": "^7.0.0",
    "oxlint": "^1.8.0",
    "patch-package": "^8.0.1",
    "playwright": "^1.54.1",
    "preact-render-to-string": "^6.5.0",
    "prop-types": "^15.8.1",
    "sade": "^1.8.1",
    "terser": "5.16.0",
    "typescript": "5.1.6",
    "undici": "^4.12.0",
    "vite": "^6.2.0",
    "vitest": "^3.2.4"
  },
  "volta": {
    "node": "20.19.1"
  }
}
