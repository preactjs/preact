{
	"compilerOptions": {
		"baseUrl": ".",
		"checkJs": true,
		"jsx": "react",
		"jsxFactory": "createElement",
		"lib": ["dom", "es5"],
		"moduleResolution": "node",
		"paths": {
			"preact": ["."],
			"preact/*": ["./*"]
		},
		"target": "es2015"
	},
	"exclude": ["node_modules", "dist", "demo"]
}
